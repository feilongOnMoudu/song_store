<!--miniprogram/pages/home/home.wxml-->
<view>
  <swiper class="swiper" 
    indicator-dots="{{true}}"
    autoplay="{{true}}"
    >
    <block wx:for="{{imageUrl}}" wx:key="*this">
      <swiper-item>
        <image src="{{item}}" class="swiper-image" />
      </swiper-item>
    </block>
    
  </swiper>
</view>
